{
  "name": "insight-viz-monorepo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "release": "bumpp --preid rc --commit \"release: v%s\" --no-push --tag",
    "release:sub": "pnpm changeset && changeset version && pnpm i && git add . && git commit -m 'chore(release): 子仓库发布新版本'",
    "clean": "rm -rf node_modules **/*/node_modules",
    "test:web-e2e": "pnpm -C packages/web test:e2e",
    "test:web-unit": "pnpm -C packages/web test",
    "clean:win": "rmdir /Q /S node_modules",
    "build:web": "pnpm -C packages/web build",
    "build:h5": "pnpm -C packages/mini-apps build:h5",
    "dev:web": "pnpm -C packages/web dev",
    "dev:h5": "pnpm -C packages/mini-apps dev:h5",
    "changeset:init": "changeset init",
    "add:husky-pre-commit": "husky add .husky/pre-commit 'pnpm lint-staged'",
    "add:husky-commitlint": "husky add .husky/commit-msg 'pnpm commitlint --config commitlint.config.js --edit $1'",
    "husky:init": "npx husky-init",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint packages/**/src/*.{jsx,js,ts,tsx,vue} --fix",
    "lint:prettier": "prettier --write --loglevel warn \"packages/**/src/**/*.{js,json,css,less,scss,vue,html,ts,tsx,md}\"",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "npm run lint"
    ]
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vueuse/core": "^9.4.0",
    "axios": "^1.3.4",
    "echarts": "^5.3.3",
    "pinia": "^2.0.28",
    "pinia-plugin-persist": "^1.0.0",
    "qs": "^6.11.0",
    "vue": "^3.2.47",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-angular": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.2",
    "@playwright/test": "^1.31.1",
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/vue": "^6.6.1",
    "@types/node": "^18.11.12",
    "@types/qs": "^6.9.7",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vitest/coverage-c8": "^0.24.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-standard": "^8.0.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.2.6",
    "@vue/tsconfig": "^0.1.3",
    "bumpp": "^9.0.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-vue": "^9.3.0",
    "happy-dom": "^7.5.10",
    "husky": "^8.0.0",
    "lint-staged": "^12.4.0",
    "npm-run-all": "^4.1.5",
    "only-allow": "^1.1.1",
    "prettier": "^2.7.1",
    "sass": "^1.50.1",
    "start-server-and-test": "^1.15.2",
    "typescript": "^4.9.3",
    "unplugin-vue-components": "^0.23.0",
    "vite": "^4.1.4",
    "vitest": "^0.25.8",
    "vue-tsc": "^1.1.7"
  }
}
