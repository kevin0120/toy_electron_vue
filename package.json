{
  "name": "toy_electron_vue",
  "version": "1.0.0",
  "author": "Kevin",
  "license": "CC0-1.0",
  "description": "Hello Electron application",
  "main": "main.js",
  "scripts": {
    "build": "webpack",
    "vue_app:install": "cd ./app/vue_app && yarn ",
    "vue_app:build": "yarn vue_app:install && pnpm -C ./app/vue_app build",
    "vue_app:serve": "yarn vue_app:install && pnpm -C ./app/vue_app serve",
    "electron:local:start": "electron ./main.js",
    "electron:vue_app:start": "yarn vue-app:build && cross-env Project_Entrance=vue_app  electron ./main.js",
    "electron:remote_odoo:start": "cross-env Project_Entrance=remote_odoo electron ./main.js",
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "repository": " https://github.com/kevin0120/toy_electron_vue.git",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "devDependencies": {
    "@electron-forge/cli": "^6.0.5",
    "@electron-forge/maker-deb": "^6.0.5",
    "@electron-forge/maker-rpm": "^6.0.5",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.5",
    "electron": "^23.1.2",
    "webpack": "^5.76.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "airspeed": "^1.0.4",
    "cross-env": "^7.0.3",
    "electron-settings": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "fs-extra": "^11.1.0",
    "vite": "^4.1.4",
    "winston": "^3.8.2"
  }
}
